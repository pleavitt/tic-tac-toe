{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-flip-move": "^3.0.2",
    "react-scripts": "1.1.5",
    "react-switch": "^3.0.4",
    "react-toggle": "^4.0.2"
  },
  "scripts": {
